<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">
  <category name="Counters">
    <block type="counter">
      <field name="name">name</field>
      <field name="start">0</field>
    </block>
    <block type="calculatedcounter">
      <field name="name">name</field>
    </block>
    <block type="sumcounter"></block>
    <block type="thiscountervalue"></block>
    <block type="namedcountervalue">
      <field name="name">name</field>
    </block>
    <block type="namedcounter">
      <field name="name">name</field>
    </block>
  </category>
  <category name="Modifiers">
    <block type="modifier">
      <field name="btnText">text</field>
    </block>
  </category>
  <category name="Conditions">
    <block type="condition"></block>
    <block type="logic_compare">
      <field name="OP">EQ</field>
    </block>
    <block type="logic_operation">
      <field name="OP">AND</field>
    </block>
    <block type="logic_negate"></block>
  </category>
  <category name="Events">
    <block type="win"></block>
    <block type="lose"></block>
    <block type="dqplayer"></block>
    <block type="tagmyteam">
      <field name="tag">tag</field>
    </block>
  </category>
  <category name="Math">
    <block type="math_arithmetic">
      <field name="OP">ADD</field>
    </block>
    <block type="math_number">
      <field name="NUM">1</field>
    </block>
  </category>
</xml>
